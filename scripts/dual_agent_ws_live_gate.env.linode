# Prefilled live-gate config for Linode host:
# 172-236-28-208.ip.linodeusercontent.com
#
# Usage:
#   set -a
#   source scripts/dual_agent_ws_live_gate.env.linode
#   set +a
#   clojure -M scripts/dual_agent_ws_live_gate.clj
#
# If 7070 is busy, set both FUTON3C_PORT and FUTON3C_PUBLIC_WS_BASE to 47070.

FUTON3C_BIND_HOST=0.0.0.0
FUTON3C_PORT=7070
FUTON3C_PUBLIC_WS_BASE=wss://172-236-28-208.ip.linodeusercontent.com:7070

FUTON3C_SESSION_ID=sess-alleycat-live
FUTON3C_WAIT_MS=300000

FUTON3C_EXTERNAL_CLAUDE=true
FUTON3C_REQUIRE_CLAUDE_ACTION=true

FUTON3C_USE_REAL_BACKEND=false
FUTON3C_REAL_BACKEND_CWD=/home/joe/code/futon3c
